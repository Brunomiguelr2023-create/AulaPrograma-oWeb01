styles_css = textwrap.dedent("""\
:root {
  --max-width: 1100px;
  --gap: 16px;
}
* { box-sizing: border-box; }
body { font-family: system-ui, sans-serif; line-height: 1.6; margin: 0; padding: 0; color: #222; }
.container { max-width: var(--max-width); margin: 0 auto; padding: var(--gap); }
header { background: #f7f7f7; border-bottom: 1px solid #ddd; }
.logo { font-weight: bold; text-decoration: none; color: #333; }
nav ul { list-style: none; display: flex; gap: 12px; padding: 0; margin: 0; }
nav a { text-decoration: none; color: #333; }
nav a[aria-current="page"] { font-weight: bold; }
main { padding: var(--gap); }
figure { margin: 1.5rem 0; }
img { max-width: 100%; height: auto; display: block; border-radius: 8px; }
form { display: grid; gap: 8px; max-width: 600px; }
fieldset { border: 1px solid #ccc; padding: 1rem; margin-bottom: 1rem; border-radius: 8px; }
legend { font-weight: bold; }
label { display: block; margin-top: 8px; }
input, select { width: 100%; padding: 8px; border: 1px solid #ccc; border-radius: 4px; }
button { padding: 10px 14px; background: #0078d4; color: white; border: none; border-radius: 6px; cursor: pointer; }
button:hover { background: #005fa3; }
footer { background: #f1f1f1; text-align: center; padding: 1rem; margin-top: 2rem; }

/* small accessibility helpers */
input:invalid { outline: 2px solid #c33; }
input:valid { outline: 2px solid #2a8f2a; }

@media (min-width: 900px) {
  main { display: grid; grid-template-columns: 2fr 1fr; gap: 24px; }
}
""")

masks_js = textwrap.dedent("""\
document.addEventListener('DOMContentLoaded', () => {
  const cpf = document.getElementById('cpf');
  const telefone = document.getElementById('telefone');
  const cep = document.getElementById('cep');

  function aplicarMascara(input, mascara) {
    if (!input) return;
    input.addEventListener('input', () => {
      let valor = input.value.replace(/\\D/g, '');
      let novoValor = '';
      let j = 0;
      for (let i = 0; i < mascara.length && j < valor.length; i++) {
        novoValor += mascara[i] === '9' ? valor[j++] : mascara[i];
      }
      input.value = novoValor;
    });
    // evitar colar texto inválido
    input.addEventListener('paste', (e) => {
      e.preventDefault();
      const text = (e.clipboardData || window.clipboardData).getData('text').replace(/\\D/g, '');
      input.value = text;
      input.dispatchEvent(new Event('input'));
    });
  }

  aplicarMascara(cpf, '999.999.999-99');
  aplicarMascara(telefone, '(99) 99999-9999');
  aplicarMascara(cep, '99999-999');
});
""")

# write files
files = {
    base / 'index.html': index_html,
    base / 'projetos.html': projetos_html,
    base / 'cadastro.html': cadastro_html,
    base / 'css' / 'styles.css': styles_css,
    base / 'js' / 'masks.js': masks_js
}
for path, content in files.items():
    path.write_text(content, encoding='utf-8')

# create simple placeholder images
def create_placeholder(path, size, text):
    img = Image.new('RGB', size, (240, 240, 240))
    draw = ImageDraw.Draw(img)
    try:
        font = ImageFont.truetype("DejaVuSans.ttf", 24)
    except Exception:
        font = ImageFont.load_default()
    w, h = draw.textsize(text, font=font)
    draw.text(((size[0]-w)/2, (size[1]-h)/2), text, fill=(60,60,60), font=font)
    img.save(path, format='JPEG', quality=85)

create_placeholder(base / 'images' / 'hero-index.jpg', (1200, 600), "Hero - Organização")
create_placeholder(base / 'images' / 'voluntariado.jpg', (1000, 500), "Voluntariado")
create_placeholder(base / 'images' / 'cadastro-banner.jpg', (900, 400), "Cadastro Banner")

# create zip
zip_path = Path('/mnt/data/semantic-site.zip')
with zipfile.ZipFile(zip_path, 'w', zipfile.ZIP_DEFLATED) as zf:
    for dirpath, dirnames, filenames in os.walk(base):
        for fname in filenames:
            fpath = Path(dirpath) / fname
            zf.write(fpath, fpath.relative_to(base.parent))

zip_size = zip_path.stat().st_size

print(f"Created project at: {base}")
print(f"Zip file: {zip_path} ({zip_size} bytes)")

# expose download link path for the user
zip_path_str = str(zip_path)
zip_path_str

STDOUT/STDERR
